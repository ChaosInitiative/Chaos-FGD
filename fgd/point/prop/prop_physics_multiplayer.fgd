	physicsmode[engine](integer) : "Physics Mode" : 0
	physicsmode(choices) : "Physics Mode" : 0 =
		[
		0: "Auto Detect"
		1: "Solid, Server-side"
		2: "Non-Solid, Server-side"
		3: "Non-Solid, Client-side"
		]
	]
